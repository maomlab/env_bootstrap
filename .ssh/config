#maomlab account
Host github.com-maomlab
        HostName github.com
        User git
        IdentityFile ~/.ssh/id_rsa_maomlab

#momeara account
Host github.com-momeara
        HostName github.com
        User git
        IdentityFile ~/.ssh/id_rsa_momeara


# To use
# For the git repo github.com/maomlab/<repo>, edit .git/config like this:
#
# [remote "origin"]
# 	  url = git@github.com-maomlab:maomlab/<repo>.git
# 	  fetch = +refs/heads/*:refs/remotes/origin/*